fun map(list, func) {
  var new_list = [];
  for (var i = 0; i < 3; i = i + 1) { // Assuming list length 3 for now
    new_list[i] = func(list[i]);
  }
  return new_list;
}

fun double(x) { return x * 2; }

var results = map([1, 2, 3], double);
print results[0]; // expect: 2
print results[1]; // expect: 4
print results[2]; // expect: 6